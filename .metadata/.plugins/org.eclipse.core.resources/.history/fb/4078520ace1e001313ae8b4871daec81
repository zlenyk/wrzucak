
public class Zadanie {
    public static void main(String[] args){
        int zero = 0;
        int one = 1;
        int two = 2;
        System.out.println(zero + one);
        System.out.println(one/two);
        System.out.println(two/(one +two));
        System.out.println((one + 2*two) % (one + 2));
    }
}