import java.nio.CharBuffer;
public class Zadanie {
public static void main(String[] args) {
CharBuffer buffer = CharBuffer.allocate(128);
buffer.put("Ala".toCharArray());
buffer.flip();
System.out.print("<" + buffer + ">");
buffer.get();
System.out.print("<" + buffer + ">");
buffer.get();
System.out.print("<" + buffer + ">");
buffer.reset();
System.out.print("<" + buffer + ">");
}
}