package cont;

import cont.LoginProtocole.ResponseCode;

public class RegistrationProtocole {

	private static final String REGISTERCODE = "REGISTER";
	ConnectionManager cm;
	
	private enum ResponseCode{
		OK,WRONG,ERROR;
	}
	
	RegistrationProtocole(){
		cm = new ConnectionManager();
	}
	
	/**
	 * @param login
	 * @param password
	 * @return result of operation
	 */
	public String start(String login,String password){
		
		if(cm.tryToConnect(login) == false){
			return ResponseCode.ERROR.name();
		}
		
		return ResponseCode.OK.name();
	}
}
